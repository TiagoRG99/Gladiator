; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Gladiator"
run/main_scene="res://TitleScreen.tscn"
config/icon="res://Sprites PI/Logo/logo[10148].png"

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false
mouse_cursor/custom_image="res://Sprites PI/normal.png"
window/stretch/mode="viewport"
window/stretch/shrink="1"

[editor_plugins]

enabled=PoolStringArray( "gut" )

[rendering]

environment/default_clear_color=Color( 0.14902, 0.192157, 0.219608, 1 )
environment/default_environment="res://default_env.tres"
